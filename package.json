    "@types/compression": "^0.0.35",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.16.0",
    "@types/faker": "^4.1.3",
    "@types/joi": "^13.4.4",
    "@types/lodash": "^4.14.116",
    "@types/mocha": "^5.2.5",
    "@types/node": "^9.6.31",
