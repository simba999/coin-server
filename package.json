{
  "name": "ishu-server",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
  },
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "test": "mocha -r ts-node/register src/**/*.spec.ts --exit --no-timeouts",
    "test-watch": "mocha -r ts-node/register src/**/*.spec.ts --no-timeouts --watch-extensions ts --watch",
    "tools": "ts-node ./src/tools.ts"
  },
  "dependencies": {
    "@machinomy/types-truffle-contract": "git+https://github.com/machinomy/types-truffle-contract.git",
    "@machinomy/types-truffle-hdwallet-provider": "git+https://github.com/machinomy/types-truffle-hdwallet-provider.git",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/nodemailer": "^4.6.2",
    "@types/passport": "^0.4.6",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/sequelize": "^4.27.25",
    "@types/swagger-jsdoc": "0.0.2",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/uuid": "^3.4.4",
    "@types/web3": "^1.0.3",
    "@types/yargs": "^11.1.1",
    "async": "^2.6.1",
    "body-parser": "^1.18.3",
    "boom": "^7.2.0",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-validator": "^4.3.0",
    "faker": "^4.1.0",
    "joi": "^13.6.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "mysql2": "^1.6.1",
    "nodemailer": "^4.6.8",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "sequelize": "^4.38.0",
    "sequelize-cli-typescript": "^3.2.0-c",
    "sequelize-typescript": "^0.6.6",
    "swagger-jsdoc": "^1.10.3",
    "swagger-ui-express": "^3.0.10",
    "truffle-contract-typescript": "^1.0.13",
    "truffle-hdwallet-provider": "0.0.6",
    "uuid": "^3.3.2",
    "web3": "^0.18.4",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "@types/async": "^2.0.49",
